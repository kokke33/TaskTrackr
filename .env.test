# テスト用環境変数
NODE_ENV=test

# テスト用データベース（明示的にユーザー指定）
DATABASE_URL=postgresql://test:test@localhost:5432/tasktrackr_test
# PostgreSQLユーザー明示
PGUSER=test
PGPASSWORD=test
PGHOST=localhost
PGPORT=5432
PGDATABASE=tasktrackr_test

# セッション設定
SESSION_SECRET=test_session_secret

# AI設定（テスト用）
AI_PROVIDER=test
AI_LOG_LEVEL=error
AI_LOG_CONSOLE=false
AI_LOG_FILE=false

# テスト用ポート (ワークフローのヘルスチェックと統一)
PORT=5000

# その他のテスト設定
VITEST=true